{
  "name": "api-clean-nodejs",
  "version": "1.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node build/main/server.js",
    "build": "tsc",
    "debug": "node --watch ./build --inspect=0.0.0.0:9222 --nolazy ./build/main/server.js",
    "up": "npm run build && sudo docker compose up",
    "down": "sudo docker compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js --detectOpenHandles",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@shelf/jest-mongodb": "^4.3.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.13",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.11.9",
    "git-commit-msg-linter": "^5.0.6",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^4.1.1",
    "validator": "^13.11.0"
  },
  "engines": {
    "node": "18.x"
  }
}